Cell["\<\
This finds the evolution equations for the effective one-body solution for a \
two-body system, using the equations from the Bern paper.\
\>", "Text",
 CellChangeTimes->{{3.829825059487234*^9, 3.829825082166504*^9}, {
  3.829825174111374*^9, 
  3.829825190960453*^9}},ExpressionUUID->"955f8819-e135-4366-8c68-\
b3c002336003"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"EOB", "[", 
    RowBox[{"r_", ",", "\[Theta]_", ",", "pr_", ",", "p\[Theta]_"}], "]"}], 
   "=", 
   RowBox[{"Hpol", "/.", 
    RowBox[{"r", "\[Rule]", 
     RowBox[{"r", 
      SqrtBox[
       RowBox[{
        SuperscriptBox[
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "2"], "+", 
        SuperscriptBox[
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "2"]}]]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rdotEOB", "=", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"EOB", "[", 
      RowBox[{"r", ",", "theta", ",", "pr", ",", "ptheta"}], "]"}], ",", 
     "pr"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Theta]dotEOB", "=", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"EOB", "[", 
      RowBox[{"r", ",", "theta", ",", "pr", ",", "ptheta"}], "]"}], ",", 
     "ptheta"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"prdotEOB", "=", 
   RowBox[{"D", "[", 
    RowBox[{
     RowBox[{"-", 
      RowBox[{"EOB", "[", 
       RowBox[{"r", ",", "theta", ",", "pr", ",", "ptheta"}], "]"}]}], ",", 
     "r"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"p\[Theta]dotEOB", "=", 
  RowBox[{"D", "[", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"EOB", "[", 
      RowBox[{"r", ",", "theta", ",", "pr", ",", "ptheta"}], "]"}]}], ",", 
    "theta"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"equationsEOB", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"EOB", "[", 
      RowBox[{"r", ",", "theta", ",", "pr", ",", "ptheta"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"rdotEOB", ",", "\[Theta]dotEOB"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"prdotEOB", ",", "p\[Theta]dotEOB"}], "}"}]}], "}"}]}], 
